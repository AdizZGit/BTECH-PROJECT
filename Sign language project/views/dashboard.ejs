<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Dashboard</title>
    <style>
        /* Add your CSS styles here */
        table {
            width: 100%;
            border-collapse: collapse;
            margin-bottom: 20px;
        }
        th, td {
            padding: 8px;
            text-align: left;
            border-bottom: 1px solid #ddd;
        }
        th {
            background-color: #f2f2f2;
        }
        .logout-link {
            position: fixed;
            top: 10px;
            right: 10px;
            background-color: #f2f2f2;
            padding: 8px 16px;
            border-radius: 5px;
            text-decoration: none;
            color: #333;
        }
    </style>
</head>
<body>
    <h1>Welcome to the Admin Dashboard</h1>
    
    <div>
        <h2>Contact Data:</h2>
        <table>
            <thead>
                <tr>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Subject</th>
                    <th>Message</th>
                </tr>
            </thead>
            <tbody>
                <% contacts.forEach(contact => { %>
                    <tr>
                        <td><%= contact.name %></td>
                        <td><%= contact.email %></td>
                        <td><%= contact.subject %></td>
                        <td><%= contact.message %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>

    <!-- Add your JavaScript code here -->

    <!-- Logout link with CSS -->
    <a href="/logout" class="logout-link">Logout</a>

</body>
</html>
