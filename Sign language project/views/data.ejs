<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link href="https://cdn.jsdelivr.net/npm/remixicon@4.1.0/fonts/remixicon.css" rel="stylesheet" />
  <link rel="shortcut icon" href="/images/favicon.png" type="image/x-icon" />
  <link rel="stylesheet" href="/css/index.css" />
  <style>
    .logout-btn {
      background-color: #f0f0f0; /* Default background color */
      color: #333; /* Default text color */
      border: 1px solid #ccc; /* Default border */
      padding: 10px 20px; /* Padding for the button */
      cursor: pointer; /* Cursor changes to pointer on hover */
      transition: background-color 0.3s, color 0.3s; /* Smooth transition for hover effect */
    }
    
    .logout-btn:hover {
      background-color: #d9534f; /* Background color on hover */
      color: #fff; /* Text color on hover */
      border: 1px solid #d43f3a; /* Border color on hover */
    }
    
    .logout-btn:active {
      background-color: #c9302c; /* Background color when clicking */
      border: 1px solid #ac2925; /* Border color when clicking */
    }
    
  </style>
  <script>

    // Function to start counting animation when element is in viewport
    function startCountAnimation(elementId, finalCount, duration) {
      var counterElement = document.getElementById(elementId);
      var count = 0;
      var increment = (finalCount - count) / duration; // Calculate the increment based on the duration
  
      // For counter2, set increment to 1 to ensure whole numbers
      if (elementId === 'counter2') {
        increment = 1;
      }
  
      var interval = setInterval(function () {
        count += increment;
        if (count >= finalCount) {
          clearInterval(interval);
          if (finalCount === 360000000) {
            counterElement.textContent = '360M+';
          } else if (finalCount === 70000000) {
            counterElement.textContent = '70M+';
          } else {
            counterElement.textContent = finalCount;
          }
        } else {
          counterElement.textContent = formatCount(count);
        }
      }, 0.5); // Adjust the interval for smoother animation
    }
  
    // Function to format the count with appropriate symbols
    function formatCount(count) {
      if (count >= 1000000 && count < 1000000000) {
        return (count / 1000000).toFixed(0) + 'M+';
      } else if (count >= 70000000 && count < 100000000) {
        return (count / 1000000).toFixed(0) + 'M+';
      } else if (count >= 100000 && count < 1000000) {
        return (count / 1000).toFixed(0) + 'K+';
      } else if (count >= 1000000000) {
        return (count / 1000000000).toFixed(0) + 'B+';
      }
      return count;
    }
  
    // Function to check if element is in viewport
    function isInViewport(element) {
      var bounding = element.getBoundingClientRect();
      return (
        bounding.top >= 0 &&
        bounding.bottom <= (window.innerHeight || document.documentElement.clientHeight)
      );
    }
  
    // Function to handle scroll event
    function handleScroll() {
      var counterSection = document.getElementById('people-counter');
      if (isInViewport(counterSection)) {
        // Duration of animation (milliseconds)
        var duration = 400; // Adjust this value as needed
        startCountAnimation('counter1', 360000000, duration);
        startCountAnimation('counter2', 130, duration);
        startCountAnimation('counter3', 70000000, duration);
        window.removeEventListener('scroll', handleScroll); // Remove event listener once animation starts
      }
    }
  
    // Add event listener for scroll event
    window.addEventListener('scroll', handleScroll);
  
  
  
    // feature images  on the home page
    // feature1
    document.addEventListener('DOMContentLoaded', function () {
      // Function to add the second image
      function addSecondImage() {
        const image2 = document.createElement('img');
        image2.id = 'image2';
        image2.src = '/images/feature-male-content-creators.jpg';
        image2.alt = '';
        image2.loading = 'lazy';
        image2.style.display = 'none'; // Initially hidden
        document.querySelector('.feature-content1-img').appendChild(image2);
      }
  
      // Function to toggle the visibility of the images
      function toggleImages() {
        const images = document.querySelectorAll('.feature-content1-img img');
        images.forEach(image => {
          if (image.style.display === 'none') {
            image.style.display = 'block';
          } else {
            image.style.display = 'none';
          }
        });
      }
  
      // Add the second image after the DOM is fully loaded
      addSecondImage();
  
      // Initially, only the first image is visible
      // After 3 seconds, the images are toggled
      setTimeout(toggleImages, 3000);
  
      // This will repeat the process every 3 seconds
      setInterval(toggleImages, 3000);
    });
    
    // feature1
    document.addEventListener('DOMContentLoaded', function () {
      // Function to add the second image
      function addSecondImage() {
        const image4 = document.createElement('img');
        image4.id = 'image4';
        image4.src = '/images/feature-learn-back.svg';
        image4.alt = '';
        image4.loading = 'lazy';
        image4.style.display = 'none'; // Initially hidden
        document.querySelector('.feature-content2-img').appendChild(image4);
      }
  
      // Function to toggle the visibility of the images
      function toggleImages() {
        const images = document.querySelectorAll('.feature-content2-img img');
        images.forEach(image => {
          if (image.style.display === 'none') {
            image.style.display = 'block';
          } else {
            image.style.display = 'none';
          }
        });
      }
  
      // Add the second image after the DOM is fully loaded
      addSecondImage();
  
      // Initially, only the first image is visible
      // After 3 seconds, the images are toggled
      setTimeout(toggleImages, 3000);
  
      // This will repeat the process every 3 seconds
      setInterval(toggleImages, 3000);
    });
  
    // go to home link
  function goToHome() {
      window.location.href = "#";
    }
    
    // nav active links
    document.addEventListener('DOMContentLoaded', function() {
      var path = window.location.pathname;
      var page = path.split("/").pop();
    
      var navLinks = document.querySelectorAll('.nav-links a');
      navLinks.forEach(function(link) {
          if (link.getAttribute('href') === page) {
              link.classList.add('active');
          }
      });
    });
    
    // navigation bar
    function showSidebar() {
      const sidebar = document.querySelector('.sidebar');
      sidebar.style.display = 'flex';
    }
    function hideSidebar() {
      const sidebar = document.querySelector('.sidebar');
      sidebar.style.display = 'none';
    }
    
    // splash screen
    document.addEventListener("DOMContentLoaded", function() {
      // Check if the splash screen has been shown before
      var splashShown = sessionStorage.getItem('splashShown');
      
      if (!splashShown) {
        // If splash screen hasn't been shown before, show it and mark as shown
        setTimeout(function() {
          transitionToMain();
          // Store in session storage that the splash screen has been shown
          sessionStorage.setItem('splashShown', 'true');
        }, 5000);
      } else {
        // If splash screen has been shown before, immediately transition to main content
        transitionToMain();
      }
    });
    
    function transitionToMain() {
      var splashScreen = document.querySelector('.splash-screen');
      var mainContent = document.querySelector('.main-content');
    
      splashScreen.style.opacity = '0';
      splashScreen.style.transition = 'opacity 1s ease';
    
      setTimeout(function() {
        splashScreen.style.display = 'none';
        mainContent.style.display = 'block';
        mainContent.style.opacity = '1';
        mainContent.style.transition = 'opacity 1s ease';
      }, 1000);
    }
    
    
    
    
    
    
  
  
  </script>
  <title>Sign Sense - Home</title>
  
</head>

<body>
  <!-- splash screen -->
  <div class="splash-screen">
    <img
      src="data:image/svg+xml;base64,"
      alt="Free Namaste  Illustration" />
    <h2>Welcome to Sign Sense</h2>
    <div class="loading-container">
      <div class="name">Intializing</div>
      <div class="dots-container">
        <div class="dot"></div>
        <div class="dot"></div>
        <div class="dot"></div>
      </div>
    </div>
  </div>
  <!-- splash screen ends -->

  <!-- header start -->
  <div class="main-content" style="display: none">
    <header>
      <nav>
        <div class="logo">
          <img onclick="goToHome()" src="/images/favicon.png" alt="" />
          <h1 onclick="goToHome()">Sign Sense</h1>
          
            <h3>Welcome!</h3>
            <div class="welcome-container">
            <h3><%= name %></h3>
            
          </div>
          <a  href="/logout" ><input style="border: #ccc; border-radius: 20px;"  type="button" value="Logout" class="logout-btn btn-primary-soft btn"></a>

        </div>
        <div class="nav-links">
          <a class="hideOnMobile" href="/tutorial">Tutorials</a>
          <a class="hideOnMobile" href="/practise">Practice</a>
          <a class="hideOnMobile" href="/signify-chat">Signify</a>
          <a class="hideOnMobile" href="/about">About</a>
          <a class="hideOnMobile" href="/contact">Contact</a>
          <i id="menu-btn" class="ri-menu-line" onclick="showSidebar()"></i>
        </div>
      </nav>
      <div class="sidebar">
        <i id="menu-close-btn" class="ri-close-line" onclick="hideSidebar()"></i>
        <a href="login">Login</a>
        <a href="tutorial">Tutorials</a>
        <a href="practise">Practice</a>
        <a href="signify-chat">Signify</a>
        <a href="about">About</a>
        <a href="contact">Contact</a>
      </div>
    </header>

    <!-- header end -->

    <!-- main content starts -->
    <main>
      <!-- promote content stats-->
      <div class="promote-content">
        <div class="promote-content1">
          <h2>
            Introducing <span>Sign Sense</span>: The Future of Interpreters <span id="blinkIt">!</span>
          </h2>
          <p>
            Sign Sense is revolutionizing the future of interpreters. A
            technologically advanced solution designed to bridge the
            communication gap for the deaf and hard of hearing community, it
            leverages cutting-edge AI and machine learning. By transforming
            sign language into real-time spoken language.
          </p>

          <button id="redirect-btn1"><a id="login-btn" href="/logout">LOGOUT</a></button>
          <button id="redirect-btn2"><a id="explore-btn" href="#feature-container">Explore Now</a></button>
        </div>
        <div class="promote-content2">
          <img src="/images/sign-illustation.jpg" alt="" />
        </div>
      </div>
      <!-- promote content ends -->
      <div class="people-counter" id="people-counter">
        <div class="people-counter1">
          <h3 id="counter1">0</h3>
          <p>People suffers from disabling hearing loss across the globe</p>
        </div>
        <div class="people-counter2">
          <h3 id="counter2">0</h3>
          <p>Different sign languages that are currently undocumented</p>
        </div>
        <div class="people-counter3">
          <h3 id="counter3">0</h3>
          <p>People worldwide use sign language as their first language</p>
        </div>
      </div>

      <!-- features -->
      <!-- feature1 -->
      <div class="feature-container2" id="feature-container">
        <div class="feature-content2">
          <h2>Unlock the Power of Sign Language: Your Gateway to Communication</h2>
          <p>
            Welcome to our innovative platform, designed to bridge the gap between the deaf and dumb community and the rest of the world. Here, you'll find a robust machine learning model that interprets sign language from scratch, allowing you to practice and improve your skills with ease. Our platform is not just about learning; it's about breaking barriers and fostering a more inclusive society.
          </p>
        </div>
        <div class="feature-content2-img">
          <img id="image3" src="/images/feature-learn.svg" alt="">
        </div>
      </div>

      <!-- feature2 -->
      <div class="feature-container1">
        <div class="feature-content1-img">
          <img id="image1" src="/images/feature-remote-meeting.gif" alt="">
        </div>
        <div class="feature-content1">
          <h2>Bring people together to make a difference.</h2>
          <p>

            Welcome to our platform, where every connection counts. Our video calling feature breaks barriers, bringing
            people together regardless of distance or ability. With seamless sign language interpretation, we empower
            the deaf and dumb community to communicate effortlessly and make meaningful connections. Join us in
            revolutionizing communication and making a difference.

          </p>
        </div>
      </div>
    </main>
    <!--  main content ends -->

    <!-- footer starts -->
    <footer class="footer">
      <p>
        &copy; SignSense | Made with ❤️ by Team Sign Sense
      </p>
      <a href="#" target="_blank"><i class="ri-facebook-box-fill"></i></a>
      <a href="#" target="_blank"><i class="ri-linkedin-box-fill"></i></a>
      <a href="#" target="_blank"><i class="ri-instagram-fill"></i></a>
    </footer>
    <!-- footer ends -->
  </div>
</body>

</html>


